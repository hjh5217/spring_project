package exam;

import java.util.Random;
import java.util.Scanner;

public class tset2 {

	public static void main(String[] args) {
		int [] arr = new int[5];
		Scanner sc = new Scanner(System.in);
		int sum=0;
		int min=0;
		int max=0;
		for (int i = 0; i < arr.length; i++) {
			int input = sc.nextInt();
			arr[i] = input;
		}
		for (int i = 0; i < arr.length; i++) {
			sum += arr[i];
			if(arr[i]<min) {
				min = arr[i];
			}
		}
	}
}
