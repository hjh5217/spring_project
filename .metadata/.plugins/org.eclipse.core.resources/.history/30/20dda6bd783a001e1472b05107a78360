package anno_p;

import org.springframework.context.annotation.Bean;


public class AnnoConfig {
	
	@Bean
	BiHandle hd1() {
		return new BiHandle("일자핸들", "산악용");
	}
	
	@Bean
	BiWheel wh1() {
		return new BiWheel("디스크",28);
	}
	
	@Bean
	BiCycle mtb(BiHandle hd1, BiWheel wh1 ) {
		return new BiCycle("삼천리", hd1, wh1);
	}
	
}
