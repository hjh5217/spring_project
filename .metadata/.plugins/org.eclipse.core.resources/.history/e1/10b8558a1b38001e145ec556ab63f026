package anno_p;

public class Coffee {

	String name;
	int price;
	CoffeeBean beans;
	Topping topping;
	
}

public void calcPrice{
	price = beans.price
}


class CoffeeBean{
	String name;
	int price;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	@Override
	public String toString() {
		return "CoffeeBean [name=" + name + ", price=" + price + "]";
	}
}


class Topping{
	String name;
	int price;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	@Override
	public String toString() {
		return "Topping [name=" + name + ", price=" + price + "]";
	}
}