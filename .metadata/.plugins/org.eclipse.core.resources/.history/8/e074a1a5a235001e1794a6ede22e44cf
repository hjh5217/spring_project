package tal;

public class Bicycle {
	int wheel;
	Handle handle;
	Bucket bucket;
	
	public Bicycle(){
		System.out.println("Bicycle 생성자");
	}
	
	@Override
	public String toString() {
		if(wheel==3 && handle.getHandleExist().equals("핸들있음") 
				&& bucket.getBucketExist().equals("바구니 없음")) {
			return "세발자전거"
		}
	}

	public int getWheel() {
		return wheel;
	}
	public void setWheel(int wheel) {
		this.wheel = wheel;
	}
	public Handle getHandle() {
		return handle;
	}
	public void setHandle(Handle handle) {
		System.out.println("핸들 생성:" + handle);
		this.handle = handle;
	}
	public Bucket getBucket() {
		return bucket;
	}
	public void setBucket(Bucket bucket) {
		System.out.println("바구니 생성:" + bucket);
		this.bucket = bucket;
	}
	
}
