package di_p;

public class Menu {

	public Breakfast order_breakfast() {
		return new Breakfast("아침");
	}
	public Breakfast order_lunch() {
		return new Breakfast("점심");
	}
	public Breakfast order_dinner() {
		return new Breakfast("저녁");
	}
	
}

class Breakfast{
	String name;

	public Breakfast(String name) {
		super();
		this.name = name;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "아침메뉴 " + name + " 나왔습니다";
	}

}

class Lunch{
	String name;

	public Lunch(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "점심메뉴 " + name + " 나왔습니다";
	}
}

class Dinner{
	String name;

	public Dinner(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Dinner [name=" + name + "]";
	}
	
}