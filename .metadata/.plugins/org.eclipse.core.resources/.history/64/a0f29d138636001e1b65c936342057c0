package di_p.main;

import java.util.Arrays;

public class myClass {
	
	StuColl [] myClass;
	int tot,avg,rank;
	
	void calc() {
		tot = 0;
		for(int i = 0; i<myClass[i].jum.length; i++) {
			avg = tot/myClass[i].jum.length;
		}
	}
	
	public StuColl[] getMyClass() {
		return myClass;
	}

	public void setMyClass(StuColl... myClass) {
		this.myClass = myClass;
	}

	@Override
	public String toString() {
		return "myClass [myClass=" + Arrays.toString(myClass) + "]";
	}
}
