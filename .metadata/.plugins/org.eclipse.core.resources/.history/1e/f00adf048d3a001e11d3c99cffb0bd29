package exam;

import java.util.Scanner;

public class scanner {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		/*
		 * int cnt = 0; int res = 1; System.out.println("숫자 10개 입력"); while(cnt<10) {
		 * int num = sc.nextInt(); res = num * res; cnt++; System.out.println(res); }
		 * System.out.println(res);
		 */
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		if(a==2 && b==4 && c==6) {
			System.out.printf("%d*%d+%d="+a*b+c,2,4,6);
		}
	}

}
