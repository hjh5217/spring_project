package di_p.main;

import java.util.Arrays;

public class myClass {
	
	StuColl [] myClass;
	int tot,avg,rank;
	
	void calc() {
		tot = 0;
		for (StuColl i : myClass) {
			tot += i;
		}
		avg = tot/jum.length;
	}
	
	public StuColl[] getMyClass() {
		return myClass;
	}

	public void setMyClass(StuColl... myClass) {
		this.myClass = myClass;
	}

	@Override
	public String toString() {
		return "myClass [myClass=" + Arrays.toString(myClass) + "]";
	}
}
