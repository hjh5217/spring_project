package exam;

public class tset2 {

	public static void main(String[] args) {
		int sum1 = 0;
		int sum2 = 0;
		for (int i = 1; i <= 100; i++) {
			if(i%2==0) {
				System.out.println(i);
				
			}
		}
		System.out.println("================");
		for (int i = 1; i <= 100; i++) {
			if(i%2!=0) {
				System.out.println(i);
			}
		}
	}

}
