package anno_p;

import org.springframework.context.annotation.Bean;

import anno_p.com.Bonche;
import anno_p.com.WorkStation;
import anno_p.com.sub1.Mouse;
import anno_p.comabc.NoteBook;

public class Computers {

	@Bean
	Mouse hp920() {
		return new Mouse("HP920","버티컬마우스");
	}
	
	@Bean
	NoteBook gm1() {
		return new NoteBook();
		
	}
	@Bean(name = "nb")
	NoteBook gm2(Bonche LG) {
		NoteBook res = new NoteBook();
		res.setName("기가바이트");
		return res;
		
	}
	
	@Bean
	WorkStation ws1() {
		//return new WorkStation("델1",asus(),null,null,hp920());
		return new WorkStation("델1",null,null,null,hp920());
	}
	
	// 매개변수로 Bean 지정 가능
	@Bean
	WorkStation ws2(Bonche bc ,Mouse hp920) {
		return new WorkStation("델2",bc,null,null,hp920);
	}
	
}
