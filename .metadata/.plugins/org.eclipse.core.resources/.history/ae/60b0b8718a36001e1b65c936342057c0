package di_p.main;

import java.util.Arrays;

public class myClass {
	
	StuColl [] myClass;
	StuColl stu0;
	StuColl stu1;
	StuColl stu2;
	StuColl stu3;
	StuColl stu4;
	public StuColl getStu1() {
		return stu1;
	}

	public void setStu1(StuColl stu1) {
		this.stu1 = stu1;
	}

	public StuColl getStu2() {
		return stu2;
	}

	public void setStu2(StuColl stu2) {
		this.stu2 = stu2;
	}

	public StuColl getStu3() {
		return stu3;
	}

	public void setStu3(StuColl stu3) {
		this.stu3 = stu3;
	}

	public StuColl getStu4() {
		return stu4;
	}

	public void setStu4(StuColl stu4) {
		this.stu4 = stu4;
	}

	public StuColl getStu5() {
		return stu5;
	}

	public void setStu5(StuColl stu5) {
		this.stu5 = stu5;
	}

	int rank;

	public void rankCalc() {
		rank = 1;
		int i = 0;
		for (StuColl you : myClass) {
			if (myClass[i].avg < you.avg) {
				rank++;
			}
			i++;
		}
	}
	
	public int getRank() {
		return rank;
	}

	public void setRank(int rank) {
		this.rank = rank;
		rankCalc();
	}

	public StuColl[] getMyClass() {
		return myClass;
	}

	public void setMyClass(StuColl... myClass) {
		this.myClass = myClass;
	}

	@Override
	public String toString() {
		return "myClass [myClass=" + Arrays.toString(myClass) + ", rank=" + rank + "]";
	}

	
}
